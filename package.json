{
  "name": "react-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "test:update-snapshot": "jest --update-snapshot",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "cy:console": "start-server-and-test start http://localhost:3000 cy:run",
    "cy:browse": "start-server-and-test start http://localhost:3000 cy:open",
    "lint-staged": "lint-staged",
    "lint": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/",
    "lint:fix": "eslint --ext .js --ext .jsx --ext .ts --ext .tsx src/ --fix"
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx}": "eslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@types/react": "16.9.30",
    "@types/react-dom": "16.9.5",
    "@testing-library/react": "10.0.2",
    "react": "16.13.1",
    "react-dom": "16.13.1"
  },
  "devDependencies": {
    "@testing-library/dom": "7.2.0",
    "@testing-library/jest-dom": "5.3.0",
    "@types/jest": "25.1.4",
    "@typescript-eslint/eslint-plugin": "2.26.0",
    "@typescript-eslint/parser": "2.26.0",
    "css-loader": "3.4.2",
    "cypress": "4.12.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "html-webpack-plugin": "4.0.3",
    "husky": "4.2.3",
    "jest": "25.2.4",
    "lint-staged": "10.1.1",
    "mini-css-extract-plugin": "0.9.0",
    "node-sass": "4.13.1",
    "prettier": "2.0.2",
    "sass-loader": "8.0.2",
    "start-server-and-test": "1.10.11",
    "style-loader": "1.1.3",
    "svg-url-loader": "5.0.0",
    "terser-webpack-plugin": "2.3.5",
    "ts-jest": "25.3.0",
    "ts-loader": "6.2.2",
    "typescript": "3.8.3",
    "webpack": "4.42.1",
    "webpack-cli": "3.3.11",
    "webpack-dev-server": "3.10.3",
    "webpack-merge": "4.2.2"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
